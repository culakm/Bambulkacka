<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/delete_all_results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onDeleteResultsClicked"
        android:text="@string/delete_all_results" />
    <Button
        android:id="@+id/delete_id_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onDeleteIdResultClicked"
        android:text="one result delete" />
    <Button
        android:id="@+id/insert_test_results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onInsertResultsClicked"
        android:text="@string/insert_test_results" />
    <fragment
        android:id="@+id/results_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        class="com.hellbilling.bambulkacka.ResultsFragment" />

</LinearLayout>